#include <stdio.h>
void Swap(int* x, int* y)
{
    int tmp = 0;   //语句
    tmp = *x;
    *x = *y;
    *y = tmp;
}
int main()
{
    int a = 0;
    int b = 0;
scanf("%d%d",&a,&b);              //输入两个值
printf("a=%d b=%d\n", a, b);

 Swap(&a, &b);                      //自定义函数
printf("交换值:a=%d b=%d", a, b);
   
    return 0;
}
